# DevCaddy Configuration
# Copy this file to .env and fill in your values

# Enable/disable DevCaddy (optional, defaults to true in development)
VITE_DEV_CADDY_ENABLED=true

# DevCaddy Supabase Configuration (required)
# Note: Use DEV_CADDY_ prefix to avoid conflicts with your app's Supabase instance
VITE_DEV_CADDY_SUPABASE_URL=https://your-project.supabase.co
VITE_DEV_CADDY_SUPABASE_ANON_KEY=your-anon-key-here
